<view class="{{['best-modal digital-keyboard-modal',(_show)?'best-modal-active':'']}}"><view data-event-opts="{{[['tap',[['cancel',['$event']]]]]}}" class="best-modal-layer" bindtap="__e"></view><view class="best-modal-content"><view class="dk-subtitle"><text>请输入支付密码</text><cmd-icon vue-id="1" type="close" size="12" color="#000000" data-event-opts="{{[['^click',[['cancel']]]]}}" bind:click="__e" bind:__l="__l"></cmd-icon></view><view class="box"><view data-event-opts="{{[['tap',[['getKeyboard',['$event']]]]]}}" class="pwd-box clearfix" bindtap="__e"><block wx:for="{{_digits}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="{{['pwd-text',(activeInput==index)?'active':'']}}">{{payPassWord[index]}}</view></block></view></view><view class="pwd-forget"><block wx:if="{{_forget}}"><text data-event-opts="{{[['tap',[['forgetPwd',['$event']]]]]}}" bindtap="__e">忘记密码？找回并完成支付</text></block></view><view class="digital-keyboard"><view class="form_edit clearfix"><block wx:for="{{digitalList}}" wx:for-item="item" wx:for-index="__i0__" wx:key="*this"><view data-event-opts="{{[['tap',[['getKeyNumber',['$0'],[[['digitalList','',__i0__,'num']]]]]]]}}" class="form_num" bindtap="__e"><view class="{{[(item.num>1&&item.num<10)?'num':'',(item.num==='0'||item.num==='1')?'special-num':'',(item.num==='')?'no-num':'']}}"><text class="number">{{item.num}}</text><text class="letter">{{item.letter}}</text></view></view></block><view data-event-opts="{{[['tap',[['deleteNum',['$event']]]]]}}" class="delete" bindtap="__e"><cmd-icon class="deleteTag" vue-id="2" type="tags" size="30" color="#000000" bind:__l="__l"></cmd-icon></view></view></view></view></view>