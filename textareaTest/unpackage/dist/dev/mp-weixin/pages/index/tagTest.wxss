
.imgContent{
	position: relative;
}
.imgContent image{
	width:323px;
	height:110px;
	border-radius:6px;
}
.imgTagList{
	position: absolute;
	z-index: 2;
	left: 20rpx;
	top: 160rpx;
	height: 36rpx;
}
.imgTag{
	display: inline-block;
	margin-right: 20rpx;
	width:80rpx;
	height: 36rpx;
}
.tagList{
	width:80rpx;
	height:36rpx;
	background:rgba(0,0,0,0.5);
	border-radius:18rpx;
	border:2rpx solid rgba(255,255,255,1);
	display: -webkit-box;
	display: -webkit-flex;
	display: -ms-flexbox;
	display: flex;
	-webkit-box-pack: center;
	-webkit-justify-content: center;
	    -ms-flex-pack: center;
	        justify-content: center;
	-webkit-box-align: center;
	-webkit-align-items: center;
	    -ms-flex-align: center;
	        align-items: center;
}
.tagList text{
	font-size:22rpx;
	font-family:PingFangSC;
	font-weight:400;
	color:rgba(255,255,255,1);
}

