<view class="content"><view class="searchTop"><view class="searchBox"><view data-event-opts="{{[['tap',[['navToSearchPage']]]]}}" class="search-content-box" bindtap="__e"><image class="serach-icon" src="/static/home-icon-search.svg"></image><text class="search-input-box">商户 | 产品 | 场景 | 地址</text></view></view><view data-event-opts="{{[['tap',[['switchShow']]]]}}" class="switchIcon" bindtap="__e"><image class="switchImg" src="{{mapIsShow?'/static/home-icon-listcheck.svg':'/static/home-icon-mapcheck.svg'}}" mode></image><text class="switchText">{{mapIsShow?'列表':'地图'}}</text></view></view><view class="map-box"><map hidden="{{!(mapIsShow)}}" style="width:100%;height:100%;" scale="{{11}}" latitude="{{latitude}}" longitude="{{longitude}}" markers="{{covers}}" data-event-opts="{{[['markertap',[['tapMaker',['$event']]]],['tap',[['popUpDisappear']]]]}}" bindmarkertap="__e" bindtap="__e"></map></view><block wx:if="{{makerIsTap}}"><cover-view class="business-detail"><cover-view class="business-detail-top"><cover-image class="business-detail-img" src="{{popUpData.imgSrc}}" data-event-opts="{{[['tap',[['navToDetailPage']]]]}}" bindtap="__e"></cover-image><cover-view class="business-detail-topRight"><cover-view data-event-opts="{{[['tap',[['navToDetailPage']]]]}}" class="business-detail-title" bindtap="__e">{{popUpData.name}}</cover-view><cover-view class="business-detail-label"><block wx:for="{{popUpData.firLabels}}" wx:for-item="item" wx:for-index="index" wx:key="*this"><cover-view class="business-detail-labelItem" id="{{item}}">{{item}}</cover-view></block></cover-view><cover-view class="business-detail-priceBox"><cover-view class="business-detail-memberPrice">{{''+("会员价 ¥ "+popUpData.sales[0].memberPrice+" ")+''}}</cover-view><cover-view class="business-detail-normalPrice">{{''+("¥ "+popUpData.sales[0].norPrice+"起 ")+''}}</cover-view></cover-view><cover-view class="business-detail-locationInfo">西安·长安 | 12.20KM</cover-view></cover-view></cover-view><cover-view class="business-detail-scene"><cover-view style="display:flex;flex-direction:row;"><cover-view class="business-detail-sceneTitle">相关场景</cover-view><cover-view data-event-opts="{{[['tap',[['navToNavPage']]]]}}" class="navBtn" bindtap="__e">导航</cover-view></cover-view><cover-view class="business-detail-sceneLabel">{{popUpDataSecLabels}}</cover-view></cover-view><cover-view class="business-detail-text">{{''+popUpData.detail+''}}</cover-view></cover-view></block><cover-view hidden="{{!(mapIsShow)}}" class="bottomNumBox">{{'已展示'+covers.length+'个结果'}}</cover-view></view>