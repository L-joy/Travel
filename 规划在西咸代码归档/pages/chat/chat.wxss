/* pages/chat/chat.wxss */
.chat_title{background-image: url("data:image/png;base64,");width:100%;
height:125rpx;
background-size:100%;
background-position:center;
background-repeat:no-repeat;
}
.user_face image{width: 90rpx;height: 90rpx;border-radius: 100rpx; }
.user_face{flex: 0 0 20%;}
.user_name{height:40rpx;color: #fff;align-items: left;width: 100%;font-size: 30rpx;}
.user_text{color: #fff;align-items: left;width: 100%;font-size: 30rpx;font-weight: 350}
.user_text{flex: 0 0 60%;}
.user_zan{color: #fff;}
.icon-zan1,.icon-zan,.icon-zan2{justify-content: center;align-items:center;text-align: center;width: 100%;font-size: 30rpx;height:40rpx;}
.chat_user_itme{margin: 5% 0;padding-bottom:10rpx;border-bottom:1px solid rgba(210, 210, 210, 0.22);}
.chat_text_1{color: #fff;}
.chat_text{margin-top: 30rpx;}
textarea{border:1px solid #fff;
height:160rpx;
margin-top:20rpx;
border-radius:10rpx;
padding:2%;
color:#fff;
font-size:28rpx;
width:96%;

}
.c_btn{position:absolute;justify-content: center;
width:130rpx;
height:56rpx;
background-color:#fff;
text-align:center;
margin-top:25rpx;
right:0.5%;-webkit-box-align:center;
-webkit-align-items:center;
align-items:center;
}
.index_box,.chat_text_btn{position: relative;}
.chat_text_btn{height: 40rpx;}
button{position: absolute;opacity: 0;
width:130rpx;
height:56rpx;
background-color:#fff;z-index: 999999;
text-align:center;
margin-top:25rpx;
right:0.5%;}